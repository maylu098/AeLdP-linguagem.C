#include <stdio.h>

int main() {
    
    float celsius, fahrenheit;

  
    printf("Informe a temperatura em Fahrenheit: ");
    scanf("%f", &fahrenheit);

    
    celsius = ((fahrenheit - 32) * 5) / 9;

   
    printf("A temperatura em Celsius Ã©: %.2f graus Celsius\n", celsius);

    return 0;
}
